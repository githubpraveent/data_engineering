{
  "name": "dev",
  "description": "Development environment",
  "api_gateway": {
    "base_url": "https://api-dev.example.com",
    "api_key": "${API_KEY_DEV}",
    "timeout": 30,
    "retry_attempts": 3
  },
  "lambda": {
    "functions": {
      "processOrder": "arn:aws:lambda:us-east-1:123456789:function:processOrder-dev",
      "sendNotification": "arn:aws:lambda:us-east-1:123456789:function:sendNotification-dev"
    },
    "region": "us-east-1"
  },
  "queues": {
    "orderQueue": "https://sqs.us-east-1.amazonaws.com/123456789/order-queue-dev",
    "notificationQueue": "https://sqs.us-east-1.amazonaws.com/123456789/notification-queue-dev"
  },
  "event_bus": {
    "name": "test-event-bus-dev",
    "region": "us-east-1"
  },
  "database": {
    "endpoint": "test-db-dev.cluster-xyz.us-east-1.rds.amazonaws.com",
    "port": 5432,
    "name": "testdb_dev",
    "use_mock": false
  },
  "downstream_services": {
    "payment_service": {
      "base_url": "https://payment-service-dev.example.com",
      "use_simulation": true,
      "simulation_port": 8080
    },
    "inventory_service": {
      "base_url": "https://inventory-service-dev.example.com",
      "use_simulation": false
    }
  },
  "async": {
    "poll_interval": 2,
    "max_wait_time": 60,
    "retry_delay": 1
  },
  "aws": {
    "region": "us-east-1",
    "profile": "dev"
  }
}

