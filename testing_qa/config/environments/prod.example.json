{
  "name": "prod",
  "description": "Production environment",
  "api_gateway": {
    "base_url": "https://api.example.com",
    "api_key": "${API_KEY_PROD}",
    "timeout": 30,
    "retry_attempts": 3
  },
  "lambda": {
    "functions": {
      "processOrder": "arn:aws:lambda:us-east-1:123456789:function:processOrder-prod",
      "sendNotification": "arn:aws:lambda:us-east-1:123456789:function:sendNotification-prod"
    },
    "region": "us-east-1"
  },
  "queues": {
    "orderQueue": "https://sqs.us-east-1.amazonaws.com/123456789/order-queue-prod",
    "notificationQueue": "https://sqs.us-east-1.amazonaws.com/123456789/notification-queue-prod"
  },
  "event_bus": {
    "name": "test-event-bus-prod",
    "region": "us-east-1"
  },
  "database": {
    "endpoint": "test-db-prod.cluster-xyz.us-east-1.rds.amazonaws.com",
    "port": 5432,
    "name": "testdb_prod",
    "use_mock": false
  },
  "downstream_services": {
    "payment_service": {
      "base_url": "https://payment-service.example.com",
      "use_simulation": false
    },
    "inventory_service": {
      "base_url": "https://inventory-service.example.com",
      "use_simulation": false
    }
  },
  "async": {
    "poll_interval": 2,
    "max_wait_time": 60,
    "retry_delay": 1
  },
  "aws": {
    "region": "us-east-1",
    "profile": "prod"
  }
}

